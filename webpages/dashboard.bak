.dashboard_containers{
	display: inline-block;
	background-color: white;
	height: 175px;
	width: 355px;
	float: left;
	margin-bottom: 5px;
	margin-left: 2.5px;
	margin-right: 2.5px;
    //cursor: pointer;
    overflow: hidden;
}

.dashboard_containers.square_container{
	height: 175px;
	width: 175px;

}
.dashboardContent {
    
    max-width: 540px;
    overflow: hidden;
    margin: auto;
    margin-top: 5px;
}
.chartTitleContainer
{
    text-align: center;
    font-family: helvetica;
    height: 10%;
    width: 100%;
    -webkit-user-select: none;
    background-color: rgba(25, 112, 179, 0.7);
    color: white;
    font-weight: bold;
    display: table;
    
}

.chartTitle
{
    display: table-cell;
    width: 100%;
    height: 100%;
    vertical-align: middle;
    
}

.canvasContainer
{
    height: 75%;
    width: 75%;
    display: block;
    margin: auto;
    margin-top: 7.3%;
}

.dashboardCanvas
{
    position: absolute;
}

.NumberContainer
{
    display: table;
    width: 100%;
    height: 100%;
    text-align: center;
}

.chartNumber
{
    display: table-cell;
    font-size: 15vw;
    font-weight: bold;.
    width: 100%;
    height: 100%;
    vertical-align: middle;
    font-family: Helvetica;
    -webkit-user-select: none;
}





@media screen and (max-width: 415px)
{
	.dashboardContent {
  		max-width: none;
  		width: 100%;

	}

	.dashboard_containers{

		width:calc(100% - 5px);
		margin-bottom: 5px;
		margin-left: 2.5px;
		margin-right: 2.5px
	}
	.dashboard_containers.square_container{
		
		width: calc(50% - 5px);
        height: auto;
	}
    
    .chartTitle
    {
        font-size: 4vw
    
    }

}

@media screen and (min-width: 590px)
{
    .dashboardContent {
  		max-width: 1000px;
	}
    
    .dashboard_containers{
        width: calc(66% - 5px);
    }
    .dashboard_containers.square_container{
		width: calc(33% - 5px);

	}
}

@media screen and (min-width: 705px )
{
    .chartNumber
    {
        font-size: 85px;
    }
}

    

